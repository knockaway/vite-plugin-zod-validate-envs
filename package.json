{
  "name": "@knockaway/vite-plugin-zod-validate-envs",
  "version": "0.1.1",
  "description": "Vite plugin to validate and transform environment variables using Zod",
  "keywords": [
    "vite",
    "vite-plugin",
    "env-var",
    "validation",
    "zod",
    "env"
  ],
  "publishConfig": {
    "access": "restricted"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "format": "prettier --single-quote --write ."
  },
  "dependencies": {
    "unconfig": "^0.6.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.7",
    "prettier": "^3.4.2",
    "typescript": "^5.5.4",
    "unbuild": "^3.3.1"
  },
  "peerDependencies": {
    "vite": "^4 || ^5",
    "zod": "^3"
  },
  "engines": {
    "node": ">=18.15"
  }
}
